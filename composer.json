{
    "name": "holyshared/peridot-temporary-plugin",
    "keywords": [ "peridot", "temporary", "plugin", "test", "temporary directory", "temporary file" ],
    "description": "Temporary file / directory plugin for peridot-php",
    "license": "MIT",
    "authors": [
        {
            "name": "holyshared",
            "email": "holy.shared.design@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "peridot-php/peridot": "~1.16",
        "ramsey/uuid": "~3.1"
    },
    "require-dev": {
        "peridot-php/peridot-dot-reporter": "~1.0",
        "expect/peridot-expect-plugin": "~3.0",
        "phpspec/prophecy": "~1.5",
        "cloak/peridot-cloak-plugin": "~2.0",
        "cloak/robo-coveralls-kit": "~2.1",
        "holyshared/robo-peridot": "~2.0",
        "codegyre/robo": "~0.6"
    },
    "autoload": {
        "psr-4": {
            "holyshared\\peridot\\temporary\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/robo spec:all",
        "coveralls": "vendor/bin/robo coveralls:upload",
        "example": "vendor/bin/robo example:basic"
    },
    "minimum-stability": "stable"
}
